/*20、从键盘上输出n个数字（n<10）,找出其中最小的数字*/
int Find_min(int n){
    int num,min;
    for(int i=1;i<=n;i++){
        cout<<"请输入第"<<i<<"个数："<<endl;
        cin>>num;
        if(i==1) min=num;
        if(min>num) min=num;
    }
    return min;
}
int main(){
    int n;
    cout<<"请输入一个小于10的输入范围:";
    cin>>n;
    cout<<"最小值为："<<Find_min(n);
    return 0;
}
