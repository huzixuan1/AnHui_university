/*23、求出分数的前20项之和：2/1、3/2、5/3、8/5……*/
int Sum_Tw(){
    float f1,f2,f,sum;
    f1=2;
    f2=1;
    sum=0;
    for(int i=0;i<20;i++){
        f=f1/f2;
        sum+=f;
        f=f1;
        f1=f+f2;
        f2=f;
        cout<<"前"<<i+1<<"项和为："<<sum<<endl;
    }
    cout<<"最终结果为："<<sum<<endl;
    return 0;
}
int main(){
    Sum_Tw();
    return 0;
}
