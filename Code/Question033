/*33、打印出由1,2,3,4四个数字组成的4位数，并统计它的个数
 * （允许该4位数中有相同的数字，例如：1111、2222、3333、4444）*/
void Ques_Arr(){
    int n,num=0;
    for(int a=1;a<=4;a++){
        for(int b=1;b<=4;b++){
            for(int c=1;c<=4;c++){
                for(int d=1;d<=4;d++){
                    n=1000*a+100*b+10*c+d;
                    cout<<"第"<<num+1<<"个数："<<n<<endl;
                    num++;
                }
            }
        }
    }
    cout<<"共有"<<num<<"个"<<endl;
}
int main(){
    Ques_Arr();
    return 0;
}
