/*35、输出30以内符合勾股定理的组合*/
void Ques_th(){
    for(int i=1;i<=30;i++){
        for(int j=1;j<=30;j++){
            for(int k=1;k<=30;k++){
                if(i*i==j*j+k*k){
                    cout<<j<<"\t"<<k<<"\t"<<i<<endl;
                }
            }
        }
    }
}
int main(){
    Ques_th();
    return 0;
}
